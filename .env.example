# Ambiente: production para Render
NODE_ENV=production

# Porta da aplicação (Render injeta automaticamente, mas pode deixar 3000)
PORT=3000

# URL de conexão do Neon (substitua no Render pelo link verdadeiro)
DATABASE_URL=postgresql://neondb_owner:SENHA_AQUI@HOST_NEON/neondb?sslmode=require

# Segredos (gere valores grandes e aleatórios no Render)
JWT_SECRET=coloque_um_valor_bem_grande_aqui
CSRF_SECRET=coloque_outro_valor_bem_grande_aqui
